{
  "name": "mobv",
  "version": "0.1.0",
  "type": "module",
  "description": "Use Vue reactive API like MobX",
  "keywords": [
    "vue",
    "reactive",
    "mobx",
    "vue3",
    "vue2"
  ],
  "author": "ivan-lee",
  "license": "MIT",
  "source": "./src/index.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.esm.js",
  "unpkg": "./dist/index.umd.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "require": "./dist/index.cjs",
    "default": "./dist/index.modern.js"
  },
  "scripts": {
    "build": "microbundle",
    "dev": "microbundle watch",
    "test": "vitest"
  },
  "devDependencies": {
    "@testing-library/vue": "^7.0.0",
    "@types/node": "^20.3.1",
    "jsdom": "^22.1.0",
    "microbundle": "^0.15.1",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3",
    "vitest": "^0.32.2",
    "vue": "^3.3.4"
  },
  "dependencies": {
    "vue-demi": "^0.14.5"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "useTabs": false,
    "arrowParens": "avoid",
    "overrides": [
      {
        "files": "package.json",
        "options": {
          "useTabs": false,
          "parser": "json-stringify"
        }
      }
    ]
  }
}
